{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "minProperties": 1,
  "properties": {
    "ds_status": {
      "type": "string",
      "enum": ["uncontacted", "in_progress", "promise_to_pay", "loan_modification", "unresponsive"]
    },
    "promise_date": {
      "type": "string",
      "format": "date"
    },
    "modification_date": {
      "type": "string",
      "format": "date"
    }
  }
}
