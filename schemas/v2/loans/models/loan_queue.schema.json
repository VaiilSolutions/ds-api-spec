{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "loans",
    "page",
    "page_count",
    "stats"
  ],
  "additionalProperties": false,
  "properties": {
    "page": {
      "type": "integer",
      "minimum": 1
    },
    "page_count": {
      "type": "integer",
      "minimum": 1
    },
    "stats": {
      "type": "object",
      "required": [
          "total_member_follow_ups",
          "total_balance",
          "avg_risk_score",
          "ptp_percentage"
      ],
      "additionalProperties": false,
      "properties": {
        "total_member_follow_ups": {
          "type": "number",
          "minimum": 0
        },
        "total_balance": {
          "type": "string"
        },
        "avg_risk_score": {
          "type": "string"
        },
        "ptp_percentage": {
          "type": "string"
        }
      }
    },
    "loans": {
      "type": "array",
      "items": {
        "type": "object",
        "patternProperties": {
          ".*": {
            "type": ["integer", "string", "number", "array", "null"]
          }
        }
      },
      "minItems": 0
    }
  }
}
