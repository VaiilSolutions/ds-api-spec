{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "organization_name",
    "logo_collapsed",
    "logo_full",
    "logo_last_updated"
  ],
  "properties": {
    "id": {
      "type": "number",
      "minimum": 0
    },
    "organization_name": {
      "type": "string",
      "minLength": 1
    },
    "logo_collapsed": {
      "type": "string"
    },
    "logo_full": {
      "type": "string"
    },
    "logo_last_updated": {
      "type": "string",
      "format": "date"
    }
  }
}
