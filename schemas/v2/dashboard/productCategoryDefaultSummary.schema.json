{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["product_category", "loan_count", "total_balance", "healthy_percent", "watchlist_percent", "intervention_percent"],
    "additionalProperties": false,
    "properties": {
      "product_category": {
        "type": "string",
        "minLength": 1
      },
      "loan_count": {
          "type": "integer",
          "minimum": 0
      },
      "total_balance": {
          "type": "number",
          "minimum": 0
      },
      "healthy_percent": {
        "type": "number",
        "minimum": 0
      },
      "watchlist_percent": {
        "type": "number",
        "minimum": 0
      },
      "intervention_percent": {
        "type": "number",
        "minimum": 0
      }
    }
  },
  "minItems": 1
}
