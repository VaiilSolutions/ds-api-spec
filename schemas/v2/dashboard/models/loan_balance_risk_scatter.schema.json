{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["member_id", "account_id", "external_member_id", "balance", "risk_score"],
    "additionalProperties": false,
    "properties": {
      "member_id": {
        "type": "integer"
      },
      "account_id": {
        "type": "integer"
      },
      "external_member_id": {
        "type": "string",
        "minLength": 1
      },
      "balance": {
        "type": "number",
        "minimum": 0
      },
      "risk_score": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      }
    }
  },
  "minItems": 1
}
