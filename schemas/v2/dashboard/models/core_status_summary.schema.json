{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["label", "count", "amount"],
    "additionalProperties": false,
    "properties": {
      "label": {
        "type": "string",
        "minLength": 1
      },
      "count": {
        "type": "integer",
        "minimum": 0
      },
      "amount": {
        "type": "number",
        "minimum": 0
      }
    }
  },
  "minItems": 1
}
