{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "minProperties": 1,
  "patternProperties": {
    "^.*$": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["month", "loan_count", "total_balance", "core_status_percent"],
        "additionalProperties": false,
        "properties": {
          "month": {
            "type": "string",
            "enum": ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"]
          },
          "loan_count": {
            "type": "integer",
            "minimum": 0
          },
          "total_balance": {
            "type": "number",
            "minimum": 0
          },
          "core_status_percent": {
            "type": "object",
            "patternProperties": {
              ".*": {
                "type": "number",
                "minimum": 0
              }
            }
          }
        }
      },
      "minItems": 13,
      "maxItems": 13
    }
  }
}
