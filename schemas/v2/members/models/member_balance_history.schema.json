{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["month", "date", "loan_balance", "deposit_balance"],
    "additionalProperties": false,
    "properties": {
      "month": {
        "type": "string",
        "enum": ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"]
      },
      "date": {
        "type": "string",
        "format": "date"
      },
      "loan_balance": {
        "type": "number",
        "minimum": 0
      },
      "deposit_balance": {
        "type": "number",
        "minimum": 0
      }
    }
  },
  "minItems": 1,
  "maxItems": 13
}
