{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["external_member_id", "email", "full_name", "phone_number", "open_date", "city", "county", "state_code", "zip"],
  "additionalProperties": false,
  "properties": {
    "external_member_id": {
      "type": "string",
      "minLength": 1
    },
    "email": {
      "type": ["string", "null"],
      "format": "email"
    },
    "full_name": {
      "type": ["string", "null"],
      "minLength": 1
    },
    "phone_number": {
      "type": ["string", "null"],
      "minLength": 1
    },
    "open_date": {
      "type": "string",
      "format": "date"
    },
    "city": {
      "type": "string",
      "minLength": 1
    },
    "county": {
      "type": "string",
      "minLength": 1
    },
    "state_code": {
      "type": "string",
      "minLength": 1
    },
    "zip": {
      "type": "string",
      "minLength": 1
    }
  }
}
