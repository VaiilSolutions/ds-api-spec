{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["month", "healthy", "watchlist", "intervention", "chargedOff", "bankruptcy"],
    "additionalProperties": false,
    "properties": {
      "month": {
        "type": "string",
        "enum": ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"]
      },
      "healthy": {
        "type": "number",
        "minimum": 0,
      },
      "watchlist": {
        "type": "number",
        "minimum": 0,
      },
      "intervention": {
        "type": "number",
        "minimum": 0,
      },
      "chargedOff": {
        "type": "number",
        "minimum": 0,
      },
      "bankruptcy": {
        "type": "number",
        "minimum": 0,
      }
    }
  },
  "minItems": 1,
  "maxItems": 13
}
