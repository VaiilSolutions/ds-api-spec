    "required": ["balance", "defaultPropensity"],
    "additionalProperties": false,
    "properties": {
      "balance": {
        "type": "number",
        "minimum": 0
      },
      "defaultPropensity": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      }
    }
  },
  "minItems": 1
}
