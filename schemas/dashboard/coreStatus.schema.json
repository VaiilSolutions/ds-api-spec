{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["status", "label", "count", "amount"],
    "additionalProperties": false,
    "properties": {
      "status": {
        "type": "string",
        "enum": ["current", "early", "late", "critical", "chargeoff", "bankruptcy"]
      },
      "label": {
        "type": "string"
      },
      "amount": {
        "type": "number",
        "minimum": 0
      },
      "count": {
        "type": "integer",
        "minimum": 0
      }
    }
  }
}
